{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "eslint-config-alloy/react",
    "plugin:prettier/recommended"
  ],
  "globals": {},
  "rules": {
    "prettier/prettier": ["error", {
      "printWidth": 120,
      "tabWidth": 2,
      "jsxBrackSameLine": false
    }],
    "indent": "off",
    "react/jsx-indent-props": "off",
    "react/jsx-curly-newline": "off",
    "eqeqeq": "off",
    "react/jsx-fragments": "off"
  }
}